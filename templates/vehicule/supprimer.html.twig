{% extends 'base.html.twig' %}

{% block title %}Hello InterventionController!{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h1>Bienvenue sur le site de gestion de matériels, d'interventions et de véhicules : le projet BICE !</h1>
        <table class="table table-striped">
            <tr>
                <th>Dénomination</th>
                <th>Numéro</th>
                <th>Immatriculation</th>
            </tr>
            <tr>
                <td>{{ v.denomination}}</td>
                <td>{{ v.numero }}</td>
                <td>{{ v.immatriculation }}</td>

            </tr>
        </table>
        <h2>Etes-vous sûr de vouloir supprimer le véhicule</h2>
        {{ form(formulaire) }}
        <a href="{{ path("app_vehicule") }}" class="btn btn-warning">Annuler</a>
    </div>
{% endblock %}