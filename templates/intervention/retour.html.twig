{% extends 'base.html.twig' %}

{% block title %}Retour d'intervention{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h1>Bienvenue sur le site de gestion de matériels, d'interventions et de véhicules : le projet BICE !</h1>
        <table class="table table-striped">
            <tr>
                <th>Date</th>
                <th>Dénomination</th>
                <th>Description</th>
            </tr>
            <tr>
                <td>{{ i.date}}</td>
                <td>{{ i.denomination }}</td>
                <td>{{ i.description }}</td>

            </tr>
        </table>
        <h2>Retour d'intervention</h2>
        {{ form(formulaire) }}
        <a href="{{ path("app_intervention") }}" class="btn btn-warning">Annuler</a>
    </div>
{% endblock %}